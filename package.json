{
  "name": "nuxtjs-fastapi",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "backend-setup": "python3 scripts/setup.py",
    "backend-dev": "./venv/bin/python -m uvicorn api.main:app --reload --host 0.0.0.0 --port 9000",
    "fastapi-dev": "pnpm backend-setup && pnpm backend-dev",
    "nuxt-dev": "nuxt dev",
    "dev": "concurrently -k -n NUXT,API -c blue,green \"pnpm nuxt-dev\" \"pnpm fastapi-dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@types/node": "20.11.28",
    "esbuild": "^0.19.12",
    "tailwindcss": "^4",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@nuxtjs/color-mode": "^3.5.2",
    "@tailwindcss/vite": "^4.1.10",
    "@vitejs/plugin-vue": "5.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "esbuild": "0.19.12",
    "motion-v": "^1.2.1",
    "nuxt": "3.10.0",
    "tailwind-merge": "^3.3.1",
    "vite": "5.2.10",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2"
  },
  "packageManager": "npm@9.9.0"
}
