{
  "name": "nuxtjs-fastapi",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "backend-setup": "python3 scripts/setup.py",
    "backend-dev": "./venv/bin/python -m uvicorn api.main:app --reload --host 0.0.0.0 --port 9000",
    "fastapi-dev": "pnpm backend-setup && pnpm backend-dev",
